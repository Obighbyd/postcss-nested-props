{
  "name": "postcss-nested-props",
  "version": "1.1.5",
  "description": "PostCSS plugin to unwrap nested properties.",
  "main": "dist/lib/plugin.js",
  "types": "dist/lib/plugin.d.ts",
  "scripts": {
    "ava": "ava",
    "build": "npm run tsc",
    "build:watch": "npm run tsc -- --watch",
    "lint": "npm run tslint",
    "prepublish": "npm run lint && npm run build",
    "tsc": "tsc",
    "test": "npm run tsc && npm run ava",
    "test:watch": "npm run ava -- --watch",
    "tslint": "tslint --project tsconfig.json"
  },
  "ava": {
    "files": [
      "dist/test/**/*.js"
    ],
    "source": [
      "dist/**/*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jedmao/postcss-nested-props.git"
  },
  "keywords": [
    "postcss",
    "postcss-plugin",
    "nested",
    "props",
    "properties",
    "css",
    "sass"
  ],
  "author": "Jed Mao <jedmao@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jedmao/postcss-nested-props/issues"
  },
  "homepage": "https://github.com/jedmao/postcss-nested-props#readme",
  "dependencies": {
    "postcss": "^5.2.15",
    "pseudo-classes": "^1.0.0",
    "pseudo-elements": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^7.0.5",
    "ava": "^0.18.2",
    "tslint": "^4.4.2",
    "typescript": "^2.2.1"
  }
}
